{
   "schemaVersion": 1,
   "id": "${mod_id}",
   "name": "${mod_name}",
   "version": "${mod_version}",
   "icon": "${mod_id}.png",
   "description": "${mod_description}",
   "contact": {
      "homepage": "${mod_homepage}",
      "issues": "${mod_issue_tracker}"
   },
   "authors": [
      "${mod_author}"
   ],
   "license": "${mod_license}",
   "environment": "*",
   "entrypoints": {
      "main": [
         "com.li64.tide.loaders.fabric.FabricEntrypoint"
      ],
      "client": [
         "com.li64.tide.loaders.fabric.FabricClientEntrypoint"
      ],
      "fabric-datagen": [
         "com.li64.tide.datagen.fabric.TideDataGenerator"
      ],
      "modmenu": [
         "com.li64.tide.compat.modmenu.ModMenuCompat"
      ]
   },
   "accessWidener": "accesswideners/tide-${mc_version}.accesswidener",
   "depends": {
      "minecraft": "${mc_version}",
      "cloth-config": "*"
   }
}